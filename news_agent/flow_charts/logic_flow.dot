digraph NewsAgent {
    rankdir=LR;
    node [shape=box, style=filled, color=lightgray];

    START -> UserInput;
    UserInput -> SearchNews;

    SearchNews -> RemoveDuplicatedNews [label="existed"];
    SearchNews -> UserInput [label="not_existed"];

    RemoveDuplicatedNews -> SummaryNews;
    SummaryNews -> END;
}
